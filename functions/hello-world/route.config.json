{
  "base": "/hello-world",
  "handler": "handler.js",
  "routes": [
    {
      "path": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "path": "/greet",
      "handler": "greet.js",
      "methods": ["GET", "POST"]
    },
    {
      "path": "/test",
      "handler": "test.js",
      "methods": ["GET"]
    },
    {
      "path": "/test-env",
      "handler": "test-env.js",
      "methods": ["GET"]
    },
    {
      "path": "/users",
      "handler": "users.js",
      "methods": ["GET", "POST"]
    },
    {
      "path": "/users/:id",
      "handler": "users.js",
      "methods": ["GET", "PUT", "DELETE"]
    },
    {
      "path": "/users/:id/posts/:postId",
      "handler": "users.js",
      "methods": ["GET"]
    }
  ]
}
