{
  "base": "/webhook-handler",
  "handler": "handler.js",
  "routes": [
    {
      "path": "/",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "path": "/github",
      "handler": "github.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "path": "/stripe",
      "handler": "stripe.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "path": "/slack",
      "handler": "slack.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "path": "/generic",
      "handler": "generic.js",
      "methods": ["POST", "OPTIONS"]
    }
  ]
}
